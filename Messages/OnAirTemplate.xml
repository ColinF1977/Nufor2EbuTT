<tt:tt xmlns:tt="http://www.w3.org/ns/ttml" xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
   xmlns:tts="http://www.w3.org/ns/ttml#styling" xmlns:ttm="http://www.w3.org/ns/ttml#metadata"
   xmlns:ebuttm="urn:ebu:tt:metadata" xmlns:ebutts="urn:ebu:tt:style" xmlns:ebuttp="urn:ebu:tt:parameters"
   xmlns:ext="http://some-extension-namespace" xml:space="default" ttp:timeBase="smpte"
   ttp:frameRate="25" ttp:frameRateMultiplier="1 1" ttp:markerMode="discontinuous"
   ttp:dropMode="nonDrop" ttp:cellResolution="50 29" xml:lang="nl"
   ebuttp:sequenceIdentifier="sequenceIdentifier"
  ebuttp:sequenceNumber="1" >
  <tt:head>
    <ttm:copyright> Copyright [BroadcasterName]</ttm:copyright>
    <tt:styling>
      <tt:metadata>
        <ebuttm:font fontFamilyName="monospace" src="urn:ebu:tt:font:teletext"/>
      </tt:metadata>
      <tt:style xml:id="defaultStyle" tts:direction="ltr" tts:fontFamily="monospace"
         tts:fontSize="1c" tts:lineHeight="1c" tts:fontStyle="normal" tts:fontWeight="normal"
         tts:textDecoration="none" tts:unicodeBidi="normal" tts:wrapOption="noWrap"
         ebutts:multiRowAlign="auto" ebutts:linePadding="0c"/>
      <tt:style xml:id="multiRowAlignLeft" ebutts:multiRowAlign="start"/>
      <tt:style xml:id="multiRowAlignCenter" ebutts:multiRowAlign="center"/>
      <tt:style xml:id="multiRowAlignRight" ebutts:multiRowAlign="end"/>
      <tt:style xml:id="textAlignLeft" tts:textAlign="start"/>
      <tt:style xml:id="textAlignCenter" tts:textAlign="center"/>
      <tt:style xml:id="textAlignRight" tts:textAlign="end"/>
      <tt:style xml:id="Red" tts:color="#FF0000" tts:backgroundColor="transparent"/>
      <tt:style xml:id="Green" tts:color="#00FF00" tts:backgroundColor="transparent"/>
      <tt:style xml:id="Yellow" tts:color="#FFFF00" tts:backgroundColor="transparent"/>
      <tt:style xml:id="Blue" tts:color="#0000FF" tts:backgroundColor="transparent"/>
      <tt:style xml:id="Magenta" tts:color="#FF00FF" tts:backgroundColor="transparent"/>
      <tt:style xml:id="Cyan" tts:color="#00FFFF" tts:backgroundColor="transparent"/>
      <tt:style xml:id="White" tts:color="#FFFFFF" tts:backgroundColor="transparent"/>
      <tt:style xml:id="doubleHeight" tts:fontSize="2c"/>
    </tt:styling>
    <tt:layout>
      <tt:region xml:id="verticalAlignTop" tts:origin="10% 10.5%" tts:extent="80% 79%"
         tts:displayAlign="before" tts:padding="0%" tts:writingMode="lrtb" tts:overflow="hidden"/>
      <tt:region xml:id="verticalAlignBottom" tts:origin="10% 10.5%" tts:extent="80% 79%"
         tts:displayAlign="after" tts:padding="0%" tts:writingMode="lrtb" tts:overflow="hidden"/>
    </tt:layout>
  </tt:head>
  <tt:body style="defaultStyle">
    <tt:div/>
  </tt:body>
</tt:tt>
